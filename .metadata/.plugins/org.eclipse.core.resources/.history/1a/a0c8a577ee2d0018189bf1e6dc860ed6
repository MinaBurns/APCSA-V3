	}

	public Maze(int size, String line)
	{
		exitFound = false;
		

	}

	public boolean hasExitPath(int r, int c)
	{
		if(r<maze.length&& r<=0&&C<maze.length&&c>=0&&maze[r][c]==1)
			if(c ==maze.length-1)
			{
				exitFound = true;
			}
			else
			{
				int save = maze[r][c];
				maze[r][c] = 0;
				hasExitPath(r+1,c);
				hasExitPath(r-1,c);
				hasExitPath(r,c+1);
				hasExitPath(r,c-1);
				
				maze[r][c] = save;//
			}

		return false;
	}

